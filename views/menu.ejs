<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Menu</title>
  </head>
  <body>
    <h1>Full Menu</h1>
    <h2 class="mains">Mains</h2>
    <ul>
    <% menu.forEach((element,index)=>{ %>  
    <% if(element.category === "mains"){ %>
      <li><%= element.name %></li>
      <% }%> 
    <%}) %>
    </ul>
    <h2 class="desserts">Desserts</h2>
    <ul>
      <% menu.forEach((element,index)=>{ %>  
      <% if(element.category === "desserts"){ %>
        <li><%= element.name %></li>
        <% }%> 
      <%}) %>
      </ul>
    <h2 class="sides">Sides</h2>
    <ul>
      <% menu.forEach((element,index)=>{ %>  
      <% if(element.category === "sides"){ %>
        <li><%= element.name %></li>
        <% }%> 
      <%}) %>
      </ul>
  </body>
</html>
